{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "plugin:@typescript-eslint/recommended",
        //新增
        "plugin:prettier/recommended"
    ],
    "overrides": [
        {
          "rules": {
              // 禁止不必要的转义字符
              "no-useless-escape": 0
          }
        }
    ], 
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        // "vue/multi-word-component-names":"off",
        "indent": "off"
    }
}
